{
  "dependencies": {
    "@types/node": "^20.12.2",
    "date-fns": "^3.6.0",
    "firebase-admin": "^11.8.0",
    "firebase-functions": "^4.9.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "eslint": "^8.9.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.25.4",
    "firebase-functions-test": "^3.1.0",
    "typescript": "^5.4.3"
  },
  "engines": {
    "node": "18"
  },
  "main": "lib/index.js",
